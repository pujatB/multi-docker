{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [{
        "name": "client",
        "image": "pujabapat/multi-client",
        "hostName": "client",
        "essential": false
    },
    {
        "name": "server",
        "image": "pujabapat/multi-server",
        "hostname": "api",
        "essential": false
    },
    {
        "name": "worker",
        "image": "pujabapat/multi-worker",
        "hostname": "worker",
        "essential": false
    },
    {
        "name": "ngnix",
        "image": "pujabapat/multi-ngnix",
        "hostname": "ngnix",
        "essential": true,
        "portMappings": [
            {
                "hostPort": 80,
                "containerPort": 80
            }
        ],
        "links": ["client", "server"]
    }
]
}

